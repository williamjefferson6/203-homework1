#include <iostream>
using namespace std;
int main()
{
	cout << 3 * 4 + 5 * 6 + 7 * -2 << endl;
	cout << 1.5 * 2.0 + (5.5 / 2) + 5 / 4 << endl;
	cout << 23 % 5 + 31 / 4 % 3 - 17 % (16 % 10) << endl;
	cout << 345 / 10 / 3 * 55 / 5 / 6 + 10 / (5 / 2.0) << endl;
	cout << bool(1 / 2 > 0 || 4 == 9 % 5 || 1 + 1 < 1 - 1) << endl;
}
