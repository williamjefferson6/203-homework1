#include <iostream>
using namespace std;
void fibonacci(int max) {
    int x1 = 0, x2 = 1, b = 0;
    cout << "Fibonacci sequence up to " << max << ":"<<endl;
    while (x1 < max) {
        cout << x1 << endl;
        b = x1 + x2;
        x1 = x2;
        x2 = b;
    }
}
int main()
{
    int max;
    cin >> max;
    fibonacci(max);
}
