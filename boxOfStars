#include <iostream>
using namespace std;
void boxOfStars(int w, int h)
{
	for (int i = 1; i <= h; i++) {
		for (int j = 1; j <= w; j++) {
			if (i == 1 || i == h) {
				cout << "*";
			}
			else {
				if (j == 1 || j == w) {
					cout << "*";
				}
				else {
					cout << " ";
				}
			}
		}
	cout << endl;
	}
}
int main()
{
	int w, h;
	cin >> w;
	cin >> h;
	boxOfStars(w, h);
}
