#include <iostream>
using namespace std;
float computeDistance(int x1, int y1, int x2, int y2)
{
	float d = sqrt((pow(x2 - x1, 2) + (pow(y2 - y1, 2))));
	return d;
}
int main()
{
	int x1, y1, x2, y2;
	cin >> x1;
	cin >> y1;
	cin >> x2;
	cin >> y2;
	cout << computeDistance(x1, y1, x2, y2) << endl;
}
